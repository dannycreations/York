{
  "name": "york",
  "version": "1.0.0",
  "main": "dist/main.js",
  "author": "dannycreations",
  "license": "GPL",
  "private": true,
  "scripts": {
    "start:watch": "npm run clean && tsx watch src/main.ts",
    "start:debug": "npm run build && node --inspect-brk .",
    "build": "npm run clean && tsc",
    "check": "tsc --noEmit",
    "clean": "pnpx rimraf -g \"*.tsbuildinfo\" \"dist\"",
    "clean:node": "pnpx rimraf -g \"*-lock*\" \"node_modules\"",
    "update": "pnpx npm-check-updates -u -x @types/node"
  },
  "dependencies": {
    "@vegapunk/core": "^1.0.53",
    "@vegapunk/request": "^1.0.42",
    "@vegapunk/struct": "^1.0.15",
    "@vegapunk/utilities": "^1.0.76",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@sapphire/ts-config": "^5.0.1",
    "@types/node": "22.x",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.18.1"
}
