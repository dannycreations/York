{
  "name": "york",
  "version": "2.0.0",
  "main": "dist/main.js",
  "author": "dannycreations",
  "license": "GPL",
  "private": true,
  "scripts": {
    "start:watch": "npm run clean && tsx watch src/main.ts",
    "start:debug": "npm run build && node --inspect-brk .",
    "build": "npm run clean && tsc",
    "check": "npm run format && tsc --noEmit",
    "format": "prettier --write .",
    "clean": "rimraf -g \"*.tsbuildinfo\" \"dist\"",
    "clean:node": "rimraf -g \"*-lock*\" \"node_modules\"",
    "update": "npm-check-updates -u -x got,@types/node",
    "prepare": "husky"
  },
  "dependencies": {
    "@vegapunk/utilities": "^1.0.79",
    "dotenv": "^17.3.1",
    "effect": "^3.19.18",
    "got": "11.x",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "user-agents": "^1.1.669",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@sapphire/ts-config": "^5.0.3",
    "@types/node": "24.x",
    "@types/user-agents": "^1.0.4",
    "@types/ws": "^8.18.1",
    "husky": "^9.1.7",
    "npm-check-updates": "^19.4.1",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.30.1"
}
